<div id="head"> <a href="/"><h1>SAWIT Educate To Empower</h1></a> </div>
<div id="viewRecord">
<div class="container">
<h3 class="text-center">Display a learner date attendance</h3>


{{#data}}
<form class="form-content" method="GET" action="/learner/viewAttendanceRecord/{{Idnumber}}"> 
  <div class="group">
    <input type="text"  name="date" value="{{date}}" readOnly/>
